<h1>Listing payment_receiveds</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Payment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @payment_receiveds.each do |payment_received| %>
  <tr>
    <td><%= payment_received.customer_id %></td>
    <td><%= payment_received.payment %></td>
    <td><%= link_to 'Show', payment_received %></td>
    <td><%= link_to 'Edit', edit_payment_received_path(payment_received) %></td>
    <td><%= link_to 'Destroy', payment_received, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Payment received', new_payment_received_path %>
